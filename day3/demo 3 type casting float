public class Demo1 {

    public static void main(String[] args) throws Exception {

        float a = 10.5f;
        //compiler reads all decimal values as double by default

        float b =(float)10.5;

        double d=10.5;
        float f=(float)d;

        System.out.println(a);
        System.out.println(b);
        System.out.println(f);

      
    }
}
