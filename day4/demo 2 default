// Datatype

/* primitive type simple values
8bites , 16bits , 32bits , 64bits
byte, shourt , int , long -> 0
float , double -> 0.0
char -> /u0000
boolean -> false
*/ 



public class Demo1 {
        static boolean z;
        static byte b;
        static float f;


    public static void main(String[] args) throws Exception {
        // C++ char -> 8bits All asci numbers are under 127< asci
        //java char -. 16 bits unicode language



        char x = '3';
        char y = 51;

        System.out.println(x);
        System.out.println(y);
        System.out.println(z);
        System.out.println(b);
        System.out.println(f);
    }
}
