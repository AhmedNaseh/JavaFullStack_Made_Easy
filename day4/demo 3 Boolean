// Datatype

/* primitive type simple values
8bites , 16bits , 32bits , 64bits
byte, shourt , int , long -> 0
float , double -> 0.0
char -> /u0000
boolean -> false
*/ 

//class type : objects -> null by def

public class Demo1 {
        static boolean z;
        static boolean y;


    public static void main(String[] args) throws Exception {

        System.out.println(z);
        System.out.println(y);
    }
}
